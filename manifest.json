{
  "manifest_version": 3,
  "name": "YouTube Watch Later Cleaner (No API)",
  "version": "2.0.3",
  "action": {
    "default_popup": "popup.html",
    "default_title": "YT Watch Later Cleaner"
  },
  "permissions": ["tabs", "scripting", "activeTab"],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://youtu.be/*",
    "*://music.youtube.com/*"
  ],
  "background": { "service_worker": "background.js" }
}
